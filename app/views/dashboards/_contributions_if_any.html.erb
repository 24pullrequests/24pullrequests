<% if current_user.contributions.year(current_year).any? %>
  <%= render :partial => 'contributions/contribution', :collection => contributions, cache: true %>
<% elsif DateTime.new(current_year, 12, 1) > DateTime.now %>
  <p>
    <%= t('dashboard.not_december') %>
  </p>
<% else %>
  <p>
    <%= t("dashboard.no_pull_requests") %>
  </p>
<% end %>
