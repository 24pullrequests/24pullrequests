(function($organisations) {
  var $new = $("<%= escape_javascript(render(@organisations)) %>").find("a[rel=tooltip]").tooltip().end();

  $organisations.find('.js-org-list')
        .append($new)

  $organisations.find(".more")
        .replaceWith(
          "<%= escape_javascript(link_to_next_page @organisations,
                                                 t('more'),
                                                 params: params.permit(:year),
                                                 remote: true,
                                                 class: 'btn btn-success more') %>"
        );
})($(".js-org-pagination"));
